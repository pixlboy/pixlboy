{
  "name": "pixlboy",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "firebase": "^8.8.1",
    "jspdf": "^2.3.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "web-vitals": "^1.1.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build:lin": "GENERATE_SOURCEMAP=false react-scripts build",
    "build:win": "set \"GENERATE_SOURCEMAP=false\" && react-scripts build",
    "deploy": "firebase deploy",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "print:resume": "cd wkhtmltopdf/bin && wkhtmltopdf --margin-left 0 --margin-right 0 --margin-top 0 --margin-bottom 0 ./xyz.html rachit-garg-resume.pdf",
    "print:cover": "cd wkhtmltopdf/bin && wkhtmltopdf --margin-left 0 --margin-right 0 --margin-top 0 --margin-bottom 0 https://pixlboy.web.app/cover rachit-garg-cover.pdf"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "node-sass": "^6.0.1",
    "react-scripts": "4.0.3"
  }
}
